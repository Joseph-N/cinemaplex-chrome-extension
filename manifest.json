{
  "manifest_version": 2,

  "name": "Cinemaplex",
  "description": "Cinemaplex provides information about movies showing in Kenyan Cinemas",
  "version": "0.0.5",
  "short_name": "Cinemas Kenya",
  "author": "Joseph Ndungu",

  "permissions": [ 
    "http://*/*",
    "https://*/*"
  ],
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "icons": {
    "16": "icon_16.png",
    "48": "icon_48.png",
    "128": "icon_128.png"
  }
}
